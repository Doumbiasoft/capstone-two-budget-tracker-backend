{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {

    "email": {
      "type": "string",
      "minLength": 6,
      "maxLength": 60,
      "format": "email"
    },
    "firstName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "lastName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "oauthId": {
      "type": "string"
    },
    "oauthProvider": {
      "type": "string"
    },
    "oauthPicture": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "firstName",
    "lastName",
    "email",
    "oauthId",
    "oauthProvider",
    "oauthPicture"
  ]
}
